# ========================================
# Maternity Backend Production Configuration
# ========================================
# Copy this file to .env and update with your actual values
# DO NOT commit .env file to git (it's in .gitignore)

# Database Password
# Use a strong password in production
DB_PASSWORD=changeme-use-strong-password

# JWT Configuration
# Generate a secure random key (min 256 bits)
# You can use: openssl rand -base64 64
JWT_SECRET=your-secret-key-change-in-production-min-256-bits-please-generate-secure-random-key
JWT_EXPIRATION=86400000

# Admin Account
# Change these credentials immediately after first login
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123-change-immediately

# WeChat Mini Program (Optional)
# Leave empty if not using WeChat integration
WECHAT_APPID=
WECHAT_SECRET=

# CORS Configuration
# Add your frontend URLs (comma-separated, no spaces)
# Example: http://129.211.167.49:3000,https://yourdomain.com
CORS_ALLOWED_ORIGINS=http://localhost:5173,http://129.211.167.49
